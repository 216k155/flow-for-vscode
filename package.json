{
	"name": "vscode-flow",
	"displayName": "Flow Language Support",
	"description": "Flow support for VS Code",
	"version": "0.0.1",
	"publisher": "",
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Language"
	],
	"activationEvents": [
		"onLanguage:flow"
	],
	"main": "./build/flowMain",
	"contributes": {
		"languages": [
			{
				"id": "flow",
				"firstLine": "^\\/\\* @flow \\*\\/$",
				"aliases": [
					"Flow"
				]
			},
			{
				"id": "flow",
				"firstLine": "^[\"|']use babel[\"|'];$",
				"aliases": [
					"Flow"
				]
			}
		],
		"grammars": [{
			"language": "flow",
			"scopeName": "source.js.jsx",
			"path": "./syntaxes/Babel Language.json"
		}],
		"snippets": [{
			"language": "flow",
			"path": "./snippets/flow.json"
		}],
		"debuggers": [{
			"type": "extensionHost",
			"enableBreakpointsFor": {
				"languageIds": [
					"flow"
				]
			}
		},{
			"type": "node",
			"enableBreakpointsFor": {
				"languageIds": [
					"flow"
				]
			}
		}]
	},
	"scripts": {
		"vscode:prepublish": "babel ./lib --out-dir=./build",
		"compile": "./compile.sh"
	},
	"dependencies": {
		"nuclide-flow-base": "0.0.34"
	},
	"devDependencies": {
		"vscode": "0.10.x",
		"babel-cli": "^6.1.4",
		"babel-plugin-transform-flow-strip-types": "^6.0.14",
		"babel-preset-es2015": "^6.1.4",
		"babel-preset-stage-2": "^6.1.2",
		"babel-polyfill": "^6.1.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flowtype/vscode-flow.git"
	},
	"bugs": {
		"url": "https://github.com/flowtype/vscode-flow/issues"
	}
}